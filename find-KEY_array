//Check key in an array
public class Main
{
 public static void main(String[] args) {
  int a[]={1,8,7,9,5};
  int k=6,f=0;
  for(int i=0;i<5;i++){
      if(a[i]==k){
          System.out.println("Key found");
          f=1;
          break;
      }
  }
  if(f==0){
      System.out.println("Key not found");
  }
  
 }
}

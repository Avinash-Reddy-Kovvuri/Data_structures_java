//Array has duplicates or not 
public class Main 
{ 
 public static void main(String[] args) { 
 int a[] = {2,1,5,2,4}; 
    int t=0; 
    for(int i=0;i<5;i++){ 
        for(int j=i+1;j<5;j++){ 
            if (a[i]==a[j]){ 
                t=1; 
                System.out.println("Duplicates found"); 
                break; 
            } 
        if(t==1) 
        break; 
        } 
    } 
    if(t==0){ 
    System.out.println("Duplicates not found");} 
 
 } 
}

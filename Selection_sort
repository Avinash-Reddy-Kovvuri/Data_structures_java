// Selection sort 
 
public class Main 
{ 
    public static void main(String[] args) { 
    int a[]={5,2,3,1,6}; 
    int i,j,k,h; 
    for(i=0;i<5;i++){ 
        int mi =a[i],v=i; 
        for(j=i+1;j<5;j++){ 
            if(a[j]<mi){ 
                mi = a[j]; 
                v = j; 
            } 
        } 
        int t=a[i]; 
        a[i] = a[v]; 
        a[v] = t;} 
        for(h=0;h<5;h++){ 
            System.out.println(a[h]); 
        } 
         
    } 
}
